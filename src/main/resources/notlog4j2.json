{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "SYSTEM_OUT",
        "PatternLayout": {
          "pattern": "%date{yyyy-MM-dd'T'HH:mm:ss.SSSXXX} %level %logger.%method:%line: %message%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": {
          "ref": "SYSTEM_OUT"
        }
      }
    }
  }
}
